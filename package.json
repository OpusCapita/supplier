{
  "name": "supplier",
  "version": "2.0.3-SNAPSHOT",
  "description": "Supplier Microservice. Provides base CRUD Rest API for basic Supplier data manipulation.",
  "scripts": {
    "start": "npm run build ; node src/server/index.js",
    "dev": "npm run setup-consul ; ${NODE_PATH}/.bin/nodemon --watch src/server src/server/index.js",
    "build": "npm run build:client",
    "build:client": "${NODE_PATH}/.bin/webpack --config ./webpack.production.config.js --progress --profile --colors --bail",
    "test": "npm run clean ; npm run setup-consul ; ${NODE_PATH}/.bin/nyc ${NODE_PATH}/.bin/mocha --recursive -R mocha-junit-reporter",
    "clean": "${NODE_PATH}/.bin/rimraf esdoc testdb.sqlite .nyc_output coverage test-results.xml",
    "setup-consul": "curl -X PUT -d 'bnp' http://${HOST_IP}:8500/v1/kv/supplier/db-init/database ; curl -X PUT -d 'root' http://${HOST_IP}:8500/v1/kv/supplier/db-init/user ; curl -X PUT -d 'notSecurePa55w0rd' http://${HOST_IP}:8500/v1/kv/supplier/db-init/password",
    "lint": "${NODE_PATH}/.bin/eslint src",
    "esdoc": "${NODE_PATH}/.bin/rimraf esdoc && esdoc -c esdoc.json",
    "domain-doc": "mkdir -p wiki/domain-doc ; ${NODE_PATH}/.bin/sq2md --path-bl=index.js -o file-per-src -p wiki/domain-doc --field-bl=createdAt,updatedAt --output-ext=.model.md src/server/db/models",
    "doc": "npm run domain-doc"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/OpusCapita/supplier"
  },
  "author": {
    "name": "Andrei Mourzenkov",
    "email": "andrei.mourzenkov@jcatalog.com"
  },
  "license": "jCatalog",
  "readme": "README.md",
  "dependencies": {
    "babel-loader": "6.4.1",
    "babel-runtime": "6.23.0",
    "bluebird": "3.5.0",
    "body-parser": "1.17.1",
    "classnames": "2.2.5",
    "consul": "0.28.0",
    "cookie-parser": "1.4.3",
    "epilogue": "0.7.1",
    "express": "4.15.2",
    "helmet": "3.5.0",
    "intel": "1.1.2",
    "jquery": "3.2.1",
    "lodash": "4.17.4",
    "morgan": "1.8.1",
    "mysql": "2.13.0",
    "ocbesbn-config": "2.x",
    "ocbesbn-db-init": "1.x",
    "ocbesbn-web-init": "0.x",
    "opuscapita-i18n": "1.0.10",
    "react": "15.4.2",
    "react-addons-test-utils": "15.4.2",
    "react-bootstrap": "0.30.8",
    "react-dom": "15.4.2",
    "react-router": "3.0.0",
    "react-select": "1.0.0-rc.3",
    "react-spinkit": "1.1.11",
    "react-utils-helper": "0.1.4",
    "request": "2.81.0",
    "request-promise": "4.2.0",
    "rimraf": "2.6.1",
    "sequelize": "3.27.0",
    "sequelize-cli": "2.4.0",
    "sequelize-fixtures": "0.5.6",
    "sequelize-to-markdown": "1.1.10",
    "superagent": "3.0.0",
    "superagent-bluebird-promise": "4.2.0",
    "umzug": "1.11.0",
    "underscore": "1.8.3",
    "underscore.string": "3.3.4",
    "validate.js": "0.11.1"
  },
  "devDependencies": {
    "babel-cli": "6.24.0",
    "babel-core": "6.24.0",
    "babel-eslint": "7.2.1",
    "babel-jest": "19.0.0",
    "babel-plugin-istanbul": "4.1.1",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-plugin-webpack-loaders": "0.9.0",
    "babel-preset-es2015": "6.24.0",
    "babel-preset-react": "6.23.0",
    "babel-preset-stage-0": "6.22.0",
    "babel-register": "6.24.0",
    "chai": "3.5.0",
    "css-loader": "0.27.3",
    "esdoc": "0.5.2",
    "esdoc-es7-plugin": "0.0.3",
    "eslint": "3.18.0",
    "eslint-config-opuscapita": "1.0.4",
    "eslint-plugin-react": "6.10.3",
    "jest-cli": "19.0.2",
    "json-loader": "0.5.4",
    "less": "2.7.1",
    "less-loader": "2.2.3",
    "mkdirp": "0.5.1",
    "mocha": "3.2.0",
    "mocha-junit-reporter": "1.13.0",
    "nodemon": "1.11.0",
    "nyc": "10.2.0",
    "source-map-loader": "0.2.0",
    "sqlite3": "3.1.8",
    "style-loader": "0.16.1",
    "webpack": "2.3.2",
    "webpack-dev-middleware": "1.10.1"
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/test/jest-babel-preprocessor.js",
    "moduleNameMapper": {
      "^.*\\.css$": "<rootDir>/test/stubs/CSSStub.js"
    },
    "modulePathIgnorePatterns": [
      "<rootDir>/esdoc"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/src/server/"
    ],
    "collectCoverage": true,
    "coverageDirectory": "coverage",
    "verbose": true
  }
}
