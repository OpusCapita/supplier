{
  "name": "supplier",
  "version": "2.0.4-SNAPSHOT",
  "description": "Supplier Microservice. Provides base CRUD Rest API for basic Supplier data manipulation.",
  "scripts": {
    "start": "node src/server/index.js",
    "dev": "npm run setup-consul ; nodemon --watch src/server src/server/index.js",
    "local": "npm run setup-consul ; NODE_ENV=development nodemon --watch src/server www/index.js",
    "dev:production": "npm run setup-consul ; npm start",
    "build": "npm run build:client",
    "build:client": "cross-env NODE_ENV=production ; webpack -p --config ./webpack.production.config.js --profile --colors --bail",
    "test": "",
    "clean": "rimraf esdoc testdb.sqlite .nyc_output coverage test-results.xml",
    "setup-consul": "sh ./setup-consul.sh",
    "lint": "eslint src",
    "esdoc": "rimraf esdoc && esdoc -c esdoc.json",
    "domain-doc": "mkdir -p wiki/domain-doc ; sq2md --path-bl=index.js -o file-per-src -p wiki/domain-doc --field-bl=createdAt,updatedAt --output-ext=.model.md src/server/db/models",
    "rest-doc": "mkdir -p wiki/rest-doc ;npx raml2md -o file-per-resource -p wiki/rest-doc --output-ext=.endpoint.md rest-doc/main.raml",
    "doc": "npm run rest-doc ; npm run domain-doc"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/OpusCapita/supplier"
  },
  "author": {
    "name": "Andrei Mourzenkov",
    "email": "andrei.mourzenkov@jcatalog.com"
  },
  "license": "jCatalog",
  "readme": "README.md",
  "dependencies": {
    "@opuscapita/db-init": "^2.0.10",
    "@opuscapita/electronic-address": "^0.0.2",
    "@opuscapita/field-validators": "^0.0.3",
    "@opuscapita/i18n": "^1.2.4",
    "@opuscapita/react-dates": "^0.2.16",
    "@opuscapita/react-loaders": "^0.1.9",
    "@opuscapita/react-reference-select": "^1.0.4",
    "@opuscapita/service-base-ui": "^1.0.83",
    "@opuscapita/web-init": "^3.0.33",
    "babel-loader": "^7.1.4",
    "babel-runtime": "^6.26.0",
    "bluebird": "^3.5.1",
    "classnames": "^2.2.5",
    "cross-env": "^4.0.0",
    "css-loader": "^0.28.11",
    "iban": "0.0.8",
    "intel": "^1.2.0",
    "json-loader": "^0.5.7",
    "less": "^2.7.3",
    "less-loader": "^4.1.0",
    "lodash": "^4.17.5",
    "ocbesbn-config": "^2.3.3",
    "ocbesbn-logger": "^1.0.5",
    "ocbesbn-redis-events": "^1.0.6",
    "react": "^15.4.2",
    "react-bootstrap": "^0.31.5",
    "react-dom": "^15.6.2",
    "react-orgchart": "^1.0.5",
    "react-select": "^1.2.1",
    "react-svg-gauge": "^1.0.8",
    "react-table": "^6.7.6",
    "sequelize": "3.30.4",
